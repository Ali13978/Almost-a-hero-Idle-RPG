    - "\u041D\u0430\u0436\u043C\u0438 \u0438 \u0443\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u0439, \u0447\u0442\u043E\u0431\u044B \u0443\u0441\u043A\u043E\u0440\u0438\u0442\u044C \u0443\u043B\u0443\u0447\u0448\u0435\u043D\u0438\u0435 \u0442\u0432\u043E\u0438\u0445 (\u043F\u043E\u0447\u0442\u0438) \u0433\u0435\u0440\u043E\u0435\u0432."
    - "\u041F\u043E\u043C\u043D\u0438: \u0432\u0441\u0435, \u043A \u0447\u0435\u043C\u0443 \u043F\u0440\u0438\u043A\u0430\u0441\u0430\u0435\u0442\u0441\u044F \u0425\u0438\u043B\u0442, \u0442\u0443\u0442 \u0436\u0435 \u043B\u043E\u043C\u0430\u0435\u0442\u0441\u044F."
    - "\u0415\u0441\u043B\u0438 \u0447\u0435\u0441\u0442\u043D\u043E, \u0434\u0435\u0440\u0435\u0432\u043D\u044F \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u043B\u0430 \u041E\u0431\u0436\u043E\u0440\u044B\u0447\u0430 \u0441\u0440\u0430\u0436\u0430\u0442\u044C\u0441\u044F, \u043F\u043E\u0442\u043E\u043C\u0443 \u0447\u0442\u043E \u0435\u0433\u043E
    \u0432\u0441\u0435 \u043D\u0435\u043D\u0430\u0432\u0438\u0434\u044F\u0442."
    - "\u0420\u0430\u0437\u043C\u0438\u043D\u0430\u0439 \u043F\u0430\u043B\u044C\u0446\u044B \u0432\u0440\u0435\u043C\u044F \u043E\u0442 \u0432\u0440\u0435\u043C\u0435\u043D\u0438!"
    - "\u041D\u0430\u0436\u043C\u0438 \u0438 \u0434\u0435\u0440\u0436\u0438, \u0447\u0442\u043E\u0431\u044B \u0431\u044B\u0441\u0442\u0440\u0435\u0435 \u043F\u043E\u0442\u0440\u0430\u0442\u0438\u0442\u044C \u043E\u0447\u043A\u0438 \u0443\u043C\u0435\u043D\u0438\u0439."
    - "\u0412 \u0441\u0443\u043D\u0434\u0443\u043A\u0435 \u043C\u043E\u0436\u0435\u0442 \u0431\u044B\u0442\u044C \u0440\u0435\u0434\u043A\u0438\u0439 \u0438\u043B\u0438 \u0434\u0430\u0436\u0435 \u043C\u0438\u0444\u0438\u0447\u0435\u0441\u043A\u0438\u0439 \u043F\u0440\u0435\u0434\u043C\u0435\u0442!"
    - "\u0418\u0437\u043C\u0435\u043D\u0435\u043D\u0438\u0435 \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u0430 \u043C\u043E\u0436\u0435\u0442 \u0443\u0432\u0435\u043B\u0438\u0447\u0438\u0442\u044C \u043A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E \u041E\u041A."
    - "\u041D\u0435\u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u044D\u0444\u0444\u0435\u043A\u0442\u044B \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u043E\u0432 \u0438\u043C\u0435\u044E\u0442 \u043C\u0430\u043A\u0441. \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u0435, \u0430 \u043D\u0435\u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u2013
    \u043D\u0435\u0442."
    - "\u0414\u043E\u0431\u0430\u0432\u044C \u0441\u0432\u043E\u0438 \u043B\u044E\u0431\u0438\u043C\u044B\u0435 \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u044B \u0432 \u0438\u0437\u0431\u0440\u0430\u043D\u043D\u043E\u0435, \u0447\u0442\u043E\u0431\u044B \u043E\u0442\u0434\u0435\u043B\u0438\u0442\u044C \u0438\u0445 \u043E\u0442 \u043E\u0441\u0442\u0430\u043B\u044C\u043D\u044B\u0445."
    - "\u0427\u0442\u043E\u0431\u044B \u0438\u0437\u043C\u0435\u043D\u0438\u0442\u044C \u0441\u0432\u043E\u044E \u043A\u043E\u043C\u0430\u043D\u0434\u0443 \u0438\u043B\u0438 \u043A\u043E\u043B\u044C\u0446\u043E, \u0442\u0435\u0431\u0435 \u043F\u043E\u043D\u0430\u0434\u043E\u0431\u0438\u0442\u0441\u044F \u043F\u0440\u0435\u0441\u0442\u0438\u0436."
    - "\u0421\u043F\u0438\u0441\u043E\u043A \u0432\u0435\u0449\u0435\u0439, \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043D\u0440\u0430\u0432\u044F\u0442\u0441\u044F \u0412\u0435\u043A\u0441, \u043E\u0447\u0435\u043D\u044C, \u043E\u0447\u0435\u043D\u044C \u043A\u043E\u0440\u043E\u0442\u043A\u0438\u0439. \u041E\u0447\u0435\u043D\u044C!"
    - "\u0414\u043E\u0431\u0440\u044F\u043A \u041B\u0435\u043D\u043D\u0438 \u043F\u043E\u0447\u0442\u0438 \u0447\u0442\u043E \u0438\u0441\u043F\u0440\u0430\u0432\u0438\u043B\u0441\u044F \u0438 \u0442\u0435\u043F\u0435\u0440\u044C \u043E\u0447\u0435\u043D\u044C \u0440\u0435\u0434\u043A\u043E \u043E\u0431\u043C\u0430\u043D\u044B\u0432\u0430\u0435\u0442
    \u043F\u0440\u043E\u0441\u0442\u043E\u0439 \u043B\u044E\u0434."
    - "\u0417\u0430\u0441\u0442\u0440\u044F\u043B? \u0411\u044B\u0442\u044C \u043C\u043E\u0436\u0435\u0442 \u043F\u043E\u0440\u0430 \u043F\u043E\u043F\u0440\u043E\u0431\u043E\u0432\u0430\u0442\u044C \u043F\u0440\u0435\u0441\u0442\u0438\u0436?"
    - "\u0417\u0430\u0441\u0442\u0440\u044F\u043B? \u041F\u043E\u043F\u0440\u043E\u0431\u0443\u0439 \u0438\u0437\u043C\u0435\u043D\u0438\u0442\u044C \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u044B."
    - "\u0420\u0435\u0434\u043A\u043E\u0441\u0442\u044C \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u0430 \u0433\u043E\u0432\u043E\u0440\u0438\u0442 \u043E \u0442\u043E\u043C, \u043D\u0430\u0441\u043A\u043E\u043B\u044C\u043A\u043E \u043E\u0441\u043E\u0431\u0435\u043D\u043D\u044B\u0435 \u0435\u0433\u043E \u0431\u043E\u043D\u0443\u0441\u044B."
    - "\u0413\u043E\u0433\u043E\u043B\u0430 \u0443\u043D\u0438\u0447\u0442\u043E\u0436\u0438\u043B \u0442\u0430\u0432\u0435\u0440\u043D\u0443 \u0421\u044D\u043C\u0430."
    - "\u041F\u043E\u043B\u0443\u0447\u0430\u0439 \u0432 \u0434\u0432\u0430 \u0440\u0430\u0437\u0430 \u0431\u043E\u043B\u044C\u0448\u0435 \u043C\u0430\u0433\u0438\u0447\u0435\u0441\u043A\u0438\u0445 \u043A\u0430\u043C\u043D\u0435\u0439 \u0437\u0430 \u043F\u0440\u0435\u0441\u0442\u0438\u0436 \u043F\u043E\u0441\u043B\u0435 200
    \u044D\u0442\u0430\u043F\u0430."
    - "\u0422\u0440\u0443\u0434\u043D\u043E\u0435 \u0438\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u0435 \u043D\u0430 \u0432\u0440\u0435\u043C\u044F? \u041F\u0440\u043E\u0434\u0432\u0438\u043D\u044C\u0441\u044F \u0447\u0443\u0442\u044C \u0434\u0430\u043B\u044C\u0448\u0435 \u0432 \u0440\u0435\u0436\u0438\u043C\u0435 \u041F\u0440\u0438\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u0435."
    - "\u041E\u0431\u0449\u0438\u0435 \u0431\u043E\u043D\u0443\u0441\u044B \u0432\u043B\u0438\u044F\u044E\u0442 \u043D\u0430 \u0442\u0432\u043E\u0438\u0445 \u0433\u0435\u0440\u043E\u0435\u0432 \u0438 \u043A\u043E\u043B\u044C\u0446\u043E."
    - "\u041F\u043E\u043F\u0440\u043E\u0431\u0443\u0439 \u0443\u043B\u0443\u0447\u0438\u0448\u044C \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u044B, \u0435\u0441\u043B\u0438 \u043D\u0435 \u043F\u043E\u043B\u0443\u0447\u0430\u0435\u0442\u0441\u044F \u043F\u0440\u043E\u0439\u0442\u0438 \u0438\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u0435 \u043D\u0430
    \u0432\u0440\u0435\u043C\u044F."
    - "\u0411\u0430-\u0414\u0430-\u0411\u0443\u043C\u0441 \u0432\u0441\u0435\u0433\u0434\u0430 \u043F\u0430\u0445\u043D\u0435\u0442 \u0431\u0430\u0440\u0431\u0435\u043A\u044E, \u0445\u043E\u0442\u044F \u0435\u0433\u043E \u043D\u0438\u043A\u0442\u043E \u043D\u0438\u043A\u043E\u0433\u0434\u0430 \u043D\u0435 \u0437\u0432\u0430\u043B
    \u043D\u0430 \u0431\u0430\u0440\u0431\u0435\u043A\u044E."
    - "\u042D\u043C\u043C\u0435\u0442 \u043D\u0435 \u043B\u044E\u0431\u0438\u0442, \u043A\u043E\u0433\u0434\u0430 \u0435\u043C\u0443 \u0433\u043B\u0430\u0434\u044F\u0442 \u0436\u0438\u0432\u043E\u0442. \u041D\u0435 \u0437\u0430\u0431\u0443\u0434\u044C \u043F\u0435\u0440\u0435\u0441\u0447\u0438\u0442\u0430\u0442\u044C \u043F\u043E\u0442\u043E\u043C
    \u043F\u0430\u043B\u044C\u0446\u044B."
    - "\u0418\u0433\u0440\u043E\u0432\u044B\u0435 \u043D\u0430\u0441\u0442\u0440\u043E\u0439\u043A\u0438 \u043C\u043E\u0436\u043D\u043E \u043F\u043E\u043C\u0435\u043D\u044F\u0442\u044C \u0432 \u043C\u0435\u043D\u044E \u0414\u043E\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0435 \u043E\u043F\u0446\u0438\u0438."
    - "\u0418\u043D\u043E\u0433\u0434\u0430 \u043E\u0431\u044B\u0447\u043D\u044B\u0439 \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442 \u043E\u0431\u043B\u0430\u0434\u0430\u0435\u0442 \u0431\u043E\u043B\u0435\u0435 \u043B\u0443\u0447\u0448\u0438\u043C\u0438 \u0445\u0430\u0440\u0430\u043A\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043A\u0430\u043C\u0438,
    \u0447\u0435\u043C \u044D\u043F\u0438\u0447\u0435\u0441\u043A\u0438\u0439."
    - "\u0411\u0430\u043B\u0430\u0431\u043E\u043B\u0443\u0441 \u043D\u0435 \u043C\u043E\u0436\u0435\u0442 \u0447\u0438\u0442\u0430\u0442\u044C, \u043F\u043E\u0442\u043E\u043C\u0443 \u0447\u0442\u043E \u043E\u043D \u043F\u043E\u0442\u0435\u0440\u044F\u043B \u043E\u0447\u043A\u0438. \u0418 \u043E\u0442\u043A\u0430\u0437\u044B\u0432\u0430\u0435\u0442\u0441\u044F
    \u0438\u0445 \u0438\u0441\u043A\u0430\u0442\u044C."
    - "\u041B\u0438\u044F \u043A\u0441\u0442\u0430\u0442\u0438 \u043E\u0447\u0435\u043D\u044C \u043D\u0435\u043F\u043B\u043E\u0445\u043E \u0432\u0438\u0434\u0438\u0442, \u0433\u043E\u043B\u043E\u0432\u0430 \u043F\u0440\u0430\u0432\u0434\u0430 \u0443 \u043D\u0435\u0435 \u0441\u043E\u0432\u0441\u0435\u043C \u043D\u0435 \u0432\u0430\u0440\u0438\u0442."
    - "\u0422\u044B \u043C\u043E\u0436\u0435\u0448\u044C \u0432\u044B\u0431\u0440\u0430\u0442\u044C \u043D\u0430\u0440\u044F\u0434\u044B \u0434\u043B\u044F \u0441\u0432\u043E\u0438\u0445 (\u043F\u043E\u0447\u0442\u0438) \u0433\u0435\u0440\u043E\u0435\u0432!"
    - "\u041D\u0435\u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u0434\u043E\u0440\u043E\u0433\u0438\u0435 \u043D\u0430\u0440\u044F\u0434\u044B \u0438\u043C\u0435\u044E\u0442 \u0434\u043E\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0435 \u0432\u0438\u0437\u0443\u0430\u043B\u044C\u043D\u044B\u0435 \u044D\u0444\u0444\u0435\u043A\u0442\u044B!"
    - "\u041A\u0440\u0430\u0441\u0430\u0432\u0447\u0438\u043A \u0414\u0436\u0438\u043C \u043E\u0431\u043E\u0436\u0430\u0435\u0442 \u043C\u0443\u0437\u044B\u043A\u0443. \u041D\u043E \u0435\u0449\u0435 \u0431\u043E\u043B\u044C\u0448\u0435 \u043E\u043D \u043E\u0431\u043E\u0436\u0430\u0435\u0442 \u0441\u0432\u043E\u0435 \u043B\u0438\u0446\u043E."
    - "\u0416\u0434\u0435\u0448\u044C \u043E\u0447\u0435\u0440\u0435\u0434\u043D\u043E\u0435 \u043C\u0438\u043D\u0438-\u0437\u0430\u0434\u0430\u043D\u0438\u0435? \u041F\u0440\u043E\u0432\u0435\u0440\u044C \u043E\u0431\u0440\u0430\u0442\u043D\u044B\u0439 \u043E\u0442\u0441\u0447\u0435\u0442 \u0432 \u043C\u0435\u043D\u044E \u0434\u043E\u0441\u0442\u0438\u0436\u0435\u043D\u0438\u0439."
    - "\u0412\u0435\u043A\u0441 \u043B\u044E\u0442\u043E \u0434\u0443\u0431\u0430\u0441\u0438\u0442 \u0432\u0440\u0430\u0433\u043E\u0432 \u043C\u043E\u043B\u043E\u0442\u043A\u043E\u043C. \u041D\u043E \u0434\u0430\u0436\u0435 \u0435\u0439 \u043D\u0443\u0436\u0435\u043D \u043E\u0442\u0434\u044B\u0445."
    - "\u0422\u044B \u043C\u043E\u0436\u0435\u0448\u044C \u043F\u043E\u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C \u0431\u043E\u043D\u0443\u0441\u044B \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u0430, \u043D\u0430\u0436\u0430\u0432 \u043D\u0430 \u0437\u043D\u0430\u0447\u043E\u043A \u00ABi\u00BB \u043D\u0430 \u0432\u043A\u043B\u0430\u0434\u043A\u0435."
    - "\u041C\u0438\u0444\u0438\u0447\u0435\u0441\u043A\u0438\u0435 \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u044B \u043D\u0435\u043B\u044C\u0437\u044F \u0438\u0437\u043C\u0435\u043D\u0438\u0442\u044C, \u043D\u043E \u0438\u0445 \u043C\u043E\u0436\u043D\u043E \u043F\u0440\u043E\u043A\u0430\u0447\u0430\u0442\u044C."
    - "(\u041F\u043E\u0447\u0442\u0438) \u0432\u0441\u0435, \u0447\u0442\u043E \u0445\u043E\u0447\u0435\u0442 \u0422\u044D\u043C \u2013 \u044D\u0442\u043E \u0447\u0442\u043E\u0431\u044B \u0421\u044D\u043C \u0435\u0435 \u043F\u0440\u043E\u0441\u0442\u0438\u043B."
    - "\u0412 \u0438\u0433\u0440\u0435 \u0432\u0441\u0435\u0433\u043E 40 \u044D\u0444\u0444\u0435\u043A\u0442\u043E\u0432 \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u043E\u0432 \u0441 \u043C\u0430\u043A\u0441\u0438\u043C\u0430\u043B\u044C\u043D\u044B\u043C \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u0435\u043C. \u0421\u043E\u0431\u0435\u0440\u0435\u0448\u044C
    \u0438\u0445 \u0432\u0441\u0435?"
    - "\u041E\u0441\u0442\u043E\u0440\u043E\u0436\u043D\u043E, \u0435\u0441\u043B\u0438 \u0442\u044B \u043C\u0435\u043D\u044F\u0435\u0448\u044C \u0430\u0440\u0442\u0435\u0444\u0430\u043A\u0442, \u043D\u0435 \u0444\u0430\u043A\u0442, \u0447\u0442\u043E \u043D\u043E\u0432\u044B\u0439 \u0431\u0443\u0434\u0435\u0442 \u043B\u0443\u0447\u0448\u0435!"
    - "\u0410\u0440\u0442\u0435\u0444\u0430\u043A\u0442\u044B \u043D\u0435 \u0431\u0443\u0434\u0443\u0442 \u0438\u043C\u0435\u0442\u044C \u044D\u0444\u0444\u0435\u043A\u0442\u043E\u0432 \u0432\u043E \u0412\u0440\u0430\u0442\u0430\u0445 \u0413\u043E\u0433\u0430."
